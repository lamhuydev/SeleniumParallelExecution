<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Bai33 Test Extent Report With Suite XML">
    <!--  Listener ở File Suite có thể áp dụng cho nhiều Class  -->
    <listeners>
        <listener class-name="listener.TestListener"></listener>
    </listeners>
    <test name="Test Extent Report">
        <classes>
            <class name="Bai33_ExtentReports.testcases.LoginTest">
                <methods>
                    <include name="loginSuccess"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>
